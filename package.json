{
  "name": "codelinelogger",
  "displayName": "codelinelogger",
  "description": "CodeLineLogger is a versatile VS Code extension that enables seamless line logging for multiple programming languages, enhancing debugging and analysis capabilities.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
        {
            "command": "extension.wrapInLog",
            "title": "Wrap in Log Statement"
        }
    ],
    "keybindings": [
          {
              "command": "extension.wrapInLog",
              "key": "ctrl+.",
              "mac": "cmd+.",
              "when": "editorTextFocus"
          }
      ]

  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
